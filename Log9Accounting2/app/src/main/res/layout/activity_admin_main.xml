<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageLog9"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="17dp"
        android:fontFamily="@font/magnificent"
        android:onClick="onCashPressed"
        android:text="Cash Transactions  "
        android:textColor="#000"
        android:textSize="24sp"
        android:id="@+id/textView5" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:onClick="onCashSummaryPressed"
        android:fontFamily="@font/magnificent"
        android:text="Cash Summary  "
        android:textColor="#039BE5"
        android:textSize="24sp" />

    <ImageView
        android:id="@+id/imageLog9"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="25dp"
        android:layout_toEndOf="@+id/textView5"
        android:layout_toRightOf="@+id/textView5"
        android:src="@drawable/cash" />

    <ImageView
        android:id="@+id/imageLog"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/imageLog9"
        android:layout_marginTop="19dp"
        android:src="@drawable/debit" />

    <TextView
        android:id="@+id/textVie5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageLog"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="11dp"
        android:layout_toEndOf="@+id/imageLog"
        android:layout_toRightOf="@+id/imageLog"
        android:fontFamily="@font/magnificent"
        android:onClick="onCardPressed"
        android:text="Debit Card Transactions  "
        android:gravity="center"
        android:textColor="#000"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/tetView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageLog"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:fontFamily="@font/magnificent"
        android:text="Debit Card Summary  "
        android:onClick="onCardSummaryPressed"
        android:textColor="#039BE5"
        android:textSize="24sp" />

    <ImageView
        android:id="@+id/imageg9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignLeft="@+id/imageLog9"
        android:layout_alignStart="@+id/imageLog9"
        android:layout_below="@+id/tetView"
        android:layout_marginTop="38dp"
        android:src="@drawable/neft" />

    <TextView
        android:id="@+id/fgd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageLog"
        android:layout_alignStart="@+id/imageLog"
        android:layout_alignTop="@+id/imageg9"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/magnificent"
        android:onClick="onNeftPressed"
        android:text="NEFT Transactions  "
        android:textColor="#000"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/teffxtView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView"
        android:layout_alignRight="@+id/textView"
        android:layout_below="@+id/fgd"
        android:onClick="onNeftSummaryPressed"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/magnificent"
        android:text="NEFT Summary  "
        android:textColor="#039BE5"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="150dp"
        android:onClick="onLogOut"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:fontFamily="@font/magnificent"
        android:textStyle="bold"
        android:layout_alignParentStart="true"
        android:layout_margin="5dp"
        android:text="LOGOUT" />

    <Button
        android:id="@+id/button6"
        android:onClick="onResetPasswordPressed"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:fontFamily="@font/magnificent"
        android:textStyle="bold"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/fgd"
        android:layout_toRightOf="@+id/fgd"
        android:text="Change Password" />

</RelativeLayout>
